{
  "manifest_version": 3,
  "name": "Shinsegae Blossom Alarm",
  "version": "1.0",
  "permissions": [
    "storage",
    "alarms",
    "cookies"
  ],
  "host_permissions": [
    "https://blossom.shinsegae.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}